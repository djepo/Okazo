<h1>Annonces list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Foreignid</th>
            <th>Foreigntimestamp</th>
            <th>Titre</th>
            <th>Texte</th>
            <th>Lien</th>
            <th>Websitecategorie</th>
            <th>Prix</th>
            <th>Codepostal</th>
            <th>Ville</th>
            <th>Horodatageparse</th>
            <th>Lastexistencecheck</th>
            <th>Existe</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('annonces_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.foreignId }}</td>
            <td>{% if entity.foreignTimestamp %}{{ entity.foreignTimestamp|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{{ entity.titre }}</td>
            <td>{{ entity.texte }}</td>
            <td>{{ entity.lien }}</td>
            <td>{{ entity.websiteCategorie }}</td>
            <td>{{ entity.prix }}</td>
            <td>{{ entity.codepostal }}</td>
            <td>{{ entity.ville }}</td>
            <td>{% if entity.horodatageparse %}{{ entity.horodatageparse|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{% if entity.lastexistencecheck %}{{ entity.lastexistencecheck|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{{ entity.existe }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('annonces_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('annonces_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('annonces_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
